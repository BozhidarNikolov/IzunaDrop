@{
}
@model IEnumerable<IzunaDrop.Data.Models.Enemy>

<ul>
    
    @if (Model.Count()==0)
    {
        <div>
            No enemies yet
        </div>
    }
    else
    {
        @foreach (var enemy in Model)
        {
            <div>
                @enemy.Name
            </div>
        }
    }
</ul>